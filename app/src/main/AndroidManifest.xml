<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elevintech.motorbroshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register.RegisterEmployee"></activity>
        <activity android:name=".Register.EnterEmployeeID" />
        <activity android:name=".Register.SelectUserType" />
        <activity android:name=".Register.RegisterOwner" />
        <activity android:name=".Employees.CreateEmployeeActivity" />
        <activity android:name=".Employees.EmployeeListActivity" />
        <activity android:name=".Register.RegisterShop" />
        <activity android:name=".Consumer.ConsumerProfileActivity" />
        <activity android:name=".Register.SelectShopActivity" />
        <activity android:name=".SplashPage.SplashPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard.DashboardActivity" />
        <activity android:name=".Register.RegisterAccount" />
        <activity android:name=".Login.LoginActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>